{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:recommended"],
  "customManagers": [
    {
      "customType": "regex",
      "fileMatch": ["\\.ts$"],
      "matchStrings": [
        "github\\.com\\/LogisticsPet\\/(?<repo>[\\w-]+)\\?ref=(?<currentValue>[\\w\\d\\.\\-]+)"
      ],
      "datasourceTemplate": "git",
      "depNameTemplate": "github.com/LogisticsPet/{{repo}}",
      "versioningTemplate": "semver"
    }
  ]
}
